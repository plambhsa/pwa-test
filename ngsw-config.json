{
	"index": "/index.html",
	"dataGroups": [
		{
			"name": "swapi",
			"urls": ["https://swapi.co/**"],
			"cacheConfig": {
				"strategy": "freshness",
				"maxSize": 100,
				"maxAge": "3d",
				"timeout": "10s"
			}
		}
	],
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": ["/assets/**"]
			}
		}
	]
}
